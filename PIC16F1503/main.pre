RA5 = 0;
run.finish1_bit = 1;
run.check1_bit = 0;
Cnt.value_led = 0;
run.smoke_bit = 0;
run.key = 0;
run.warn_bit = 0;
while(!RA3)
{
asm("clrwdt");
RC3 = 0;
}
}
}

