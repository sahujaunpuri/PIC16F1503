}
Cnt.Count = 0;
run.power_bit = 1;
while(!RA3)
asm("clrwdt");
}

void sleep_deal()
{
if(run.sleep_bit)
{
INTCON = 0x08;
TMR1IE = 0;
TMR1ON = 0;
