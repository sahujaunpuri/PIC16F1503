INTCON = 0xC0;
TMR1IE = 1;
TMR1ON = 1;
Cnt.Count = 0;
run.sleep_bit = 0;
}
}


void key_deal()
{
if(RA3==0)
{
Cnt.DelayTime = 0;
if(run.press_bit==0&&++Cnt.presstime>=99)
{
run.key = 1;
Cnt.presstime = 0;
run.press_bit = 1;
if(run.power_bit)
{
run.wait_bit = 1;
run.check1_bit = 0;
}
Cnt.keydn++;
}
if(Cnt.keydn>=3)
{
Cnt.keydn = 0;
if(run.power_bit)
{
run.poweroff_bit = 1;
power();
}
else
{
run.poweron_bit = 1;
power();
}
}
if(run.power_bit&&run.press_bit)
{
Cnt.Count = 0;
}
}
if(RA3)
{
run.lock_bit = 0;
run.press_bit = 0;
run.wait_bit = 0;
run.show_bit = 0;
if(run.smoke_bit)
{
run.finish1_bit = 1;
run.close_bit = 1;
RC3 = 0;
run.check1_bit = 0;
Cnt.Chgdelay1 = 0;
}
run.key = 0;
RC3 = 0;

run.smoke_bit = 0;
}
}


void power()
{
uchar i = 0;
if(run.poweron_bit)
{
RA4 = 0;
RA5 = 0;
for(i=0;i<6;i++)
{
asm("clrwdt");

RA4=~RA4;
RA5=~RA5;
delay(200);
}
Cnt.Count = 0;
Cnt.value_led = 0;
run.poweron_bit = 0;
run.power_bit = 1;
}
if(run.poweroff_bit)
{
RA4 = 0;
RA5 = 0;
for(i=0;i<6;i++)
{
asm("clrwdt");

RA4=~RA4;
RA5=~RA5;
delay(200);
}
Cnt.Count = 0;
run.smoke_bit= 0;
Cnt.value_led = 0;
run.poweroff_bit = 0;
run.power_bit = 0;
run.sleep_bit = 1;
}
}


void smoke_deal()
{
if(run.power_bit&&run.press_bit)
{
if(!run.lock_bit)
{

