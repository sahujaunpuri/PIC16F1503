}
if(TempVAD>1556)
{
run.LEDB_bit = 1;
}
else
{
run.LEDB_bit = 0;
}
}



void charge_deal()
{
if(!run.press_bit&&!run.finish1_bit)
{
uchar i;
if(RA0||RA2)
{
if(!run.USB_bit&&!run.HOT_bit)
{
RC0 = 1;
run.power_bit = 0;
RA4 = 0;
RA5 = 0;
if(RA2)
{
USBPWM();
run.USB_bit = 1;
}
else
{
HOTPWM();
run.HOT_bit = 1;
}
for(i=0;i<6;i++)
{
Cnt.Count = 0;
asm("clrwdt");
delay(200);
RA4=~RA4;
RA5=~RA5;
}
uint BT;
BT = GetBT();
if(BT<1556)
{
Cnt.Count = 0;
RA5 = 0;
RA4 = 1;
}
else
{
Cnt.Count = 0;
RA4 = 0;
RA5 = 1;
}
Cnt.Count = 0;
run.sec_bit = 1;
run.sec2_bit = 1;
}
while(1)
{
asm("clrwdt");
Cnt.Count = 0;

if(run.finish_bit)
{
if(!run.over_bit)
{
uint I;
if(run.USB_bit)
{
PWM1CON = 0x00;
delay(20);
RC5 = 1;
delay(20);
I = GetUsbI();
if(I>=286)
{
PWM1DCH = 0x3E;
PWM1DCL = 0x00;
PWM1CON = 0xE0;
}
else
{
run.over_bit = 1;
}
}
if(run.HOT_bit)
{
PWM2CON = 0x00;
delay(20);
RC3 = 1;
delay(20);
I = GetIsense();
if(I>=200)
{
PWM2DCH = 0x5D;
PWM2DCL = 0x00;
PWM2CON = 0xE0;

}
else
{
run.over_bit = 1;
}
}
}
if(run.USB_bit)
{
PWM1CON = 0x00;
RC5 = 0;
}
if(run.HOT_bit)
{
PWM2CON = 0x00;
RC3 = 0;
}
delay(100);

if(run.HOT_bit&&GetBT()>=2007)
{
run.complete = 1;
}
if(run.USB_bit&&GetBT()>=1966)
{
run.complete = 1;
}
if(run.complete)
{
uchar i;
run.complete = 0;
Cnt.Count = 0;
run.over_bit = 0;
run.finish_bit = 0;
Cnt.ChgCount = 0;
run.sec_bit = 0;
RA4 = 0;
RA5 = 0;
for(i=0;i<42;i++)
{
Cnt.Count = 0;
asm("clrwdt");
delay(150);
RA5=~RA5;
}
RA5 = 0;
run.HOT_bit = 0;
run.USB_bit = 0;
run.power_bit = 1;
run.sleep_bit = 1;
Cnt.Count = 0;
PWM1CON = 0x00;
PWM2CON = 0x00;
RC3 = 0;
RC5 = 0;
return;
}
if(GetBT()<1556)
{
RA5 = 0;
RA4 = 1;
}
else
{
RA4 = 0;
RA5 = 1;
}
if(!RA0&&!RA2)
{
break;
}
if(run.USB_bit)
{
if(run.over_bit)
RC5 = 1;
else
PWM1CON = 0xE0;
}
if(run.HOT_bit)
{
if(run.over_bit)
RC3 = 1;
else
PWM2CON = 0xE0;
}
run.finish_bit = 0;
}
}
run.wait_bit = 0;
run.over_bit = 0;
run.finish_bit = 0;
Cnt.ChgCount = 0;
run.USB_bit = 0;
run.HOT_bit = 0;
run.complete = 0;
run.power_bit = 1;
run.sec_bit = 0;
PWM1CON = 0x00;
PWM2CON = 0x00;
RC3 = 0;
RC5 = 0;
}
}
